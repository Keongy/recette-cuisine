pes', {\r\n                method: 'post',\r\n                body: data\r\n            })\r\n            dispatch({ type: 'ADD_RECIPE', payload: recipe })\r\n        }),\r\n        deselectRecipe: function () {\r\n            dispatch({ type: 'DESELECT_RECIPE' })\r\n        }\r\n    }\r\n}","D:\\PROGRAMMATION\\GITHUB\\recette-cuisine\\RecipeApi\\front\\src\\ui\\Modal.jsx",[],"D:\\PROGRAMMATION\\GITHUB\\recette-cuisine\\RecipeApi\\front\\src\\App\\Recipes\\Recipe.jsx",[],"D:\\PROGRAMMATION\\GITHUB\\recette-cuisine\\RecipeApi\\front\\src\\hooks\\index.js",["76"],"import { useCallback, useState } from \"react\";\r\n\r\nexport function useToggle(initial = false) {\r\n    const [state, setstate] = useState(initial)\r\n    \r\n    return [state, useCallback(\r\n        () => setstate(state => !state))]\r\n}",{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","replacedBy":"80"},{"ruleId":"77","replacedBy":"81"},{"ruleId":"79","replacedBy":"82"},{"ruleId":"83","severity":1,"message":"84","line":55,"column":23,"nodeType":"85","endLine":55,"endColumn":34},{"ruleId":"83","severity":1,"message":"84","line":6,"column":20,"nodeType":"85","endLine":6,"endColumn":31},"no-native-reassign",["86"],"no-negated-in-lhs",["87"],["86"],["87"],"react-hooks/exhaustive-deps","React Hook useCallback does nothing when called with only one argument. Did you forget to pass an array of dependencies?","Identifier","no-global-assign","no-unsafe-negation"]